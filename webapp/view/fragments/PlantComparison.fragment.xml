<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.data="sap.viz.ui5.data"
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds">
	<viz:VizFrame id="vizPlantComparison" uiConfig="{applicationSet:'fiori'}" vizType="column" width="100%" height="400px">
		<viz:dataset>
			<viz.data:FlattenedDataset data="{plantChartModel>/}">
				<viz.data:dimensions>
					<viz.data:DimensionDefinition name="Category" value="{plantChartModel>Category}"/>
				</viz.data:dimensions>
				<viz.data:measures>
					<viz.data:MeasureDefinition name="Actual Production" value="{plantChartModel>Plant1Actual}"/>
				</viz.data:measures>
			</viz.data:FlattenedDataset>
		</viz:dataset>
		<viz:feeds>
			<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="Category"/>
			<viz.feeds:FeedItem uid="valueAxis" type="Measure" values="Actual Production"/>
		</viz:feeds>
	</viz:VizFrame>
</core:FragmentDefinition>